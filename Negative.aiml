<aiml version="1.0.1" encoding="UTF-8">
<category>
<pattern>I * to *</pattern>
<template> I donot <star index = "1"/> to <star index = "2"/>
</template>
</category>

<category>
<pattern>I * at *</pattern>
<template> I donot <star index = "1"/> at <star index = "2"/>
</template>
</category>

<category>
<pattern>I * in *</pattern>
<template> I donot <star index = "1"/> in <star index = "2"/>
</template>
</category>
</aiml>
